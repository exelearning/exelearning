<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="robots" content="noindex">
        <title>{% block title %}{% endblock %}</title>
        <link rel="icon" href="{{ asset("favicon.ico") }}">
        {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        {# include own css #}
        <link rel="stylesheet" href="{{ asset('style/server.css') }}">
        {% endblock %}
        <link class="exe" rel="stylesheet" href="{{ asset('style/workarea/custom.css') }}">
        <script class="exe" src="{{ asset('libs/jquery/jquery.min.js') }}"></script>
        {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        {% endblock %}
        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body>
        {% block body %}{% endblock %}
        <script src="{{ asset('app/workarea/custom.js') }}"></script>
    </body>
</html>
